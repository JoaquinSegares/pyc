---
interface Props {
  id?: string;
}

const { id } = Astro.props;
---
<section id="precios">
    <main class=" bg-[#69a3b0]">
        <h3 class="flex justify-center text-center font-bold text-4xl p-7 bg-[#69a3b0] text-white">Precios</h3>
        
        <div class="flex flex-col sm:flex-row sm:space-x-4 space-y-4 sm:space-y-0 items-center justify-center p-4">
          <img src="public/images/CLASSIC BAGS PRECIO.jpeg" alt="Image 1" class="w-full sm:w-auto h-auto max-w-sm">
          <img src="public/images/BIG BAGS PRECIOS.jpeg" alt="Image 2" class="w-full sm:w-auto h-auto max-w-sm">
          <img src="public/images/COOL STUFF PRECIO.jpeg" alt="Image 3" class="w-full sm:w-auto h-auto max-w-sm">
        </div>
        
    
      </main>
</section>

